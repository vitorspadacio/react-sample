{
  "name": "sample",
  "version": "1.0.0",
  "description": "Exemplo para aplicações React que integraram o novo ambiente integrado para cliente ConectCar.",
  "scripts": {
    "start": "webpack serve --config config/webpack.dev.config.js --hot --history-api-fallback --env PLATFORM='local' --env VERSION='stag'",
    "build": "webpack --config config/webpack.prod.config.js --progress --env PLATFORM='production' --env VERSION='stag'",
