{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "module": "commonjs",
    "noImplicitAny": false,
    "outDir": "./dist/",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "es5",
  },
  "include": [
    "./src/**/*",
    "./**/*"
  ]
}
